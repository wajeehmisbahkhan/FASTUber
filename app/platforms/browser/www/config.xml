<?xml version='1.0' encoding='utf-8'?>
<widget id="com.fast.uber" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>FASTUber</name>
    <description>
        A transportation app for FASTians.
    </description>
    <author email="k173673@nu.edu.pk" href="http://phonegap.com">
        FASTUber Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="*" />
    <allow-navigation href="*" />
    <universal-links>
        <host name="fastuber.page.link" scheme="https" />
        <host name="uber-dfd1c.firebaseapp.com" scheme="https">
            <path url="/__/auth/callback" />
        </host>
    </universal-links>
    <preference name="AndroidLaunchMode" value="singleTask" />
</widget>
